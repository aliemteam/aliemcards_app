{
  "expo": {
    "name": "ALiEM Cards",
    "description": "ALiEM Cards App",
    "slug": "aliemcards_app",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "version": "0.2.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*",
      "data/media/**/*"
    ],
    "packagerOpts": {
      "assetExts": ["png"]
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aliem.aliemcardsapp"
    },
    "android": {
      "package": "com.aliem.aliemcardsapp"
    }
  }
}
